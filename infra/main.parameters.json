{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "resourceGroupName": {
      "value": "aiobx-aioedgeai-rg"
    },
    "virtualMachineSize": {
      "value": "${AZURE_ENV_VIRTUALMACHINESIZE}"
    },
    "virtualMachineName": {
      "value": "${AZURE_ENV_VIRTUALMACHINENAME}"
    },
    "adminUsername": {
      "value": "${AZURE_ENV_ADMINUSERNAME}"
    },
    "adminPasswordOrKey": {
      "value": "${AZURE_ENV_ADMINPASSWORDORKEY}"
    },
    "authenticationType": {
      "value": "${AZURE_ENV_AUTHENTICATIONTYPE}"
    },
    "arcK8sClusterName": {
      "value": "${AZURE_ENV_ARCK8SCLUSTERNAME}"
    },
    "scriptURI": {
      "value": "${AZURE_ENV_SCRIPTURI}"
    },
    "ShellScriptName": {
      "value": "${AZURE_ENV_SHELLSCRIPTNAME}"
    },
    "customLocationRPSPID":{
      "value": "${AZURE_ENV_CUSTOMLOCATIONRPSPID}"
    },
    "spAppId": {
      "value": "${AZURE_ENV_SPAPPID}"
    },
    "spAppObjectId": {
      "value": "${AZURE_ENV_SPAPPOBJECTID}"
    },
    "spObjectId": {
      "value": "${AZURE_ENV_SPOBJECTID}"
    },
    "spSecret": {
      "value": "${AZURE_ENV_SPSECRET}"
    },
    "tags": {
      "value": {
        "Owner": "AI Team",
        "Project": "AIOEdgeAI",
        "Environment": "Dev",
        "Toolkit": "Bicep"
      }
    }
  }
}